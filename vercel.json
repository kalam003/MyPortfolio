{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
    {
      "src": "*.pdf",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|pdf|ico|svg))",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Home.html"
    }
  ]
}